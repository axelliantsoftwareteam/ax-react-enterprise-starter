{
  "name": "ax-react-enterprise-starter",
  "version": "0.1.0",
  "private": true,
  "description": "Axelliant Software Engineering enterprise-grade Next.js + TypeScript starter",
  "license": "MIT",
  "engines": {
    "node": ">=20.11"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5",
    "vitest": "^1.5.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "eslint --fix"
    ],
    "*.{md,css,json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
